{   
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "env": {
        "browser": true,
        "node": true
      },
    "globals": {
        "it": true
    },
    "rules": {
        "react/display-name": "off",        
        "react/prop-types": "warn"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}